-- enemy
enemies={}

function newenemy(x,y)
	local o={
			x=x*8,
			y=y*8,
			sprt=5,
			tiles={},
			speed = 1,
   target_tile_i = 0,
   moving = false,
   mytimer = make_timer(0.1),
   recalculatenext=false,
	}
	
	function o:update()
			if self.mytimer:update() then
	    	self.recalculatenext=true	    	
	  end
	  
	  if self.recalculatenext then
	    	self:recalculate()		    	
	  end
			
			self:follow_path()
	end
	
	function o:recalculate()
			if self.moving then
					return
			end
			
			self.tiles = o:calculate_tile_line(ply:getcenter(),{0},7)
		 self.target_tile_i = #self.tiles > 0 and 1 or 0
		 self.recalculatenext=false
	end
	
	function o:follow_path()
   if #self.tiles == 0 or self.target_tile_i > #self.tiles then
			    self.moving = false
			    return
   end

   local tile=self.tiles[self.target_tile_i]
		 local tx,ty=tile.x*8,tile.y*8

   -- check if we're at this tile position already
   if flr(self.x) == tx and flr(self.y) == ty then
       self.target_tile_i += 1
       -- done with this tile, will target the next one in next update
       return
   end

   -- determine movement direction
   if self.x ~= tx then
       if self.x < tx then
           self.x += self.speed
           if self.x > tx then self.x = tx end
       elseif self.x > tx then
           self.x -= self.speed
           if self.x < tx then self.x = tx end
       end
       self.moving = true
   elseif self.y ~= ty then
       if self.y < ty then
           self.y += self.speed
           if self.y > ty then self.y = ty end
       elseif self.y > ty then
           self.y -= self.speed
           if self.y < ty then self.y = ty end
       end
       self.moving = true
   else
       -- already there
       self.moving = false
   end
	end
	
	-- calculate a line of tile positions
	-- stops if hitting a tile with flag f
	-- or after max distance md (in tiles)
	function o:calculate_tile_line(pos1,f,md)
    local tiles = {}

    -- convert to tile coords
    local tx, ty = flr(self.x/8), flr(self.y/8)
    local tfx, tfy = flr(pos1.x/8), flr(pos1.y/8)

    local dx = abs(tfx - tx)
    local dy = abs(tfy - ty)
    local sx = tx < tfx and 1 or -1
    local sy = ty < tfy and 1 or -1

    local err = dx - dy
    local dist = 0

    while true do
        -- store current tile
        add(tiles, {x=tx, y=ty})

        -- stop if tile has flag f
        if flgmtch(mget(tx, ty),f) then
            return {}
        end

        -- stop if at target tile
        if tx == tfx and ty == tfy then
            break
        end

        -- stop if distance limit reached
        dist += 1
        if dist >= md then
            return {}
        end

        local e2 = 2 * err
        if e2 > -dy then
            err -= dy
            tx += sx
        elseif e2 < dx then
            err += dx
            ty += sy
        end
    end

    return tiles
	end
	
	function o:draw()
			draw_tile_line(self.tiles)
			spr(self.sprt,self.x,self.y)
	end
	
	return o
end

