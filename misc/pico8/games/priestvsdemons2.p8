butarr[0]=0
dirx={-1,1, 0,0,-0.7, 0.7,0.7,-0.7}
diry={ 0,0,-1,1,-0.7,-0.7,0.7, 0.7}
		
ply={
	dirsprt={
 	anim({1,2},1,0.1),
 	anim({1,2},1,0.1),
 	anim({3,4},3,0.1),
 	anim({1,2},1,0.1),
 	anim({3,4},3,0.1),
 	anim({3,4},3,0.1),
 	anim({1,2},1,0.1),
 	anim({1,2},1,0.1)
 },
 dirf={1,0,0,0,1,0,0,1},
 box={x=1,y=1,w=6,h=6},
 dir,
 lastdir
}

function ply:init()
		self.sprt=1
		self.x=63
		self.y=63
		self.fx=false
 	self.fy=false
end

function ply:getcenter()
		return {x=self.x+4,y=self.y+4}
end

function ply:createbit()
		self.bit = 0b1111
		
		if collideflag(self.x-1,self.y,self.box,{0}) then
						-- cantmoveleft 
				self.bit=self.bit&0b1110
		end		 
		if collideflag(self.x+1,self.y,self.box,{0}) then
				-- cantmoveright
				self.bit=self.bit&0b1101
		end		 
		if collideflag(self.x,self.y-1,self.box,{0}) then
				-- cantmoveup
				self.bit=self.bit&0b1011
		end		
		if collideflag(self.x,self.y+1,self.box,{0}) then
				-- cantmovedown
				self.bit=self.bit&0b0111
		end
end

function ply:control() 
		self.dir=butarr[btn()&self.bit]	
  if self.lastdir!=self.dir and self.dir>=5 then
	  self.x=flr(self.x)+0.5
	  self.y=flr(self.y)+0.5
	 end	 
	 if self.dir>0 then
	  self.x+=dirx[self.dir]
	  self.y+=diry[self.dir]
  end
	 self.lastdir=self.dir
end

function ply:anim()
		self.sprt=1
		
		if self.dir==0 then
	  	return
  end
  
  self.sprt=updateanimation(self.dirsprt[self.dir]).sprt
	 self.fx=self.dirf[self.dir]==1
end

function ply:update()
		self:createbit()
		self:control()
		self:anim()
end

function ply:draw()
		spr(self.sprt,self.x,self.y,1,1,self.fx,self.fy)
		--draw_hitbox(self,11)
end
