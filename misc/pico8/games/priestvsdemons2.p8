-- states
game={}

function game:init()
		ply:init()
				--move to level code
			add(enemies,newenemy(2,2))
			add(enemies,newenemy(3,2))
			add(enemies,newenemy(4,2))
	end

	function game:update()
			foreach(enemies,function(e)
					e:update()
			end)
			ply:update()
	end

	function game:draw()
			map()
			ply:draw()
			foreach(enemies,function(e)
					e:draw()
		end)
end
