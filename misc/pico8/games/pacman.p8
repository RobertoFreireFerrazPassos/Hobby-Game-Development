enemies={}
grid = {}

function get_enemy_tile_grid()
  grid = {}
  for y=1,16 do
    grid[y] = {}
    for x=1,16 do
        grid[y][x] = 0
    end
  end
    
  foreach(enemies,function(e)
	  local tx0 = flr(e.x/8)
	  local ty0 = flr(e.y/8)
	  local tx1 = flr((e.x+7)/8)
	  local ty1 = flr((e.y+7)/8)
	  
	  for ty = ty0,ty1 do
     grid[ty] = grid[ty] or {}
     for tx = tx0,tx1 do
         grid[ty][tx] = e.id
     end
	  end
  end)
end

function setfood()
	for y=1,14 do
   for x=1,14 do
     if grid[y][x]==0 and fget(mget(x,y))!=1 then
       mset(x+mappos.x*16,y+mappos.y*16,16)
     end
   end
	end
end


function draw_grid()
	for y,row in pairs(grid) do
   for x,val in pairs(row) do
     if val then
       rect(x*8,y*8,x*8+7,y*8+7,val)
     end
   end
	end
end

function newenemy(x,y,id)
	local o={
			x=x*8+mappos.x*16*8,
			y=y*8+mappos.y*16*8,
			id=id,
			sprt=rnd({33,34,35}),
			tiles={},
			speed = 1,
   target_tile_i = 0,
   moving = false,
   mytimer = make_timer(1),
   recalculatenext=false,
   box={x=0,y=0,w=8,h=8}
	}
	
	function o:update()
			if self.mytimer:update() then
	    	self.recalculatenext=true	    	
	  end
	  
	  if self.recalculatenext then
	    	self:recalculate()		    	
	  end
			
			self:follow_path()
	end
	
	function o:recalculate()
			if self.moving then
					return
			end
			
			self.tiles = o:calculate_tile_line(ply:getcenter(),{0},5)
		 self.target_tile_i = #self.tiles > 0 and 1 or 0
		 self.recalculatenext=false
	end
	
	function o:follow_path()
   if #self.tiles == 0 or self.target_tile_i > #self.tiles then
			    self.moving = false
			    return
   end

   local tile=self.tiles[self.target_tile_i]
		 local tx,ty=tile.x*8,tile.y*8			
			local crttile=grid[tile.y][tile.x]
			
			-- stop if tile has enemy
			if crttile>0 and 
						crttile!=self.id then
						self.recalculatenext=true
						self.moving=false
						return
			end
						
   -- check if we're at this tile position already
   if flr(self.x) == tx and flr(self.y) == ty then
       self.target_tile_i += 1
       -- done with this tile, will target the next one in next update
       return
   end

   -- determine movement direction
   if self.x ~= tx then
       if self.x < tx then
           self.x += self.speed
           if self.x > tx then self.x = tx end
       elseif self.x > tx then
           self.x -= self.speed
           if self.x < tx then self.x = tx end
       end
       self.moving = true
   elseif self.y ~= ty then
       if self.y < ty then
           self.y += self.speed
           if self.y > ty then self.y = ty end
       elseif self.y > ty then
           self.y -= self.speed
           if self.y < ty then self.y = ty end
       end
       self.moving = true
   else
       -- already there
       self.moving = false
   end
	end
	
	-- calculate a line of tile positions
	-- stops if hitting a tile with flag f
	-- or after max distance md (in tiles)
	function o:calculate_tile_line(pos1,f,md)
    local tiles = {}

    -- convert to tile coords
    local tx, ty = flr((self.x-mappos.x*16*8)/8), flr((self.y-mappos.y*16*8)/8)
    local tfx, tfy = flr((pos1.x-mappos.x*16*8)/8), flr((pos1.y-mappos.y*16*8)/8)

    local dx = abs(tfx - tx)
    local dy = abs(tfy - ty)
    local sx = tx < tfx and 1 or -1
    local sy = ty < tfy and 1 or -1

    local err = dx - dy
    local dist = 0

    while true do
        -- store current tile
        add(tiles, {x=tx, y=ty})

        -- stop if tile has flag f
        if flgmtch(mget(tx, ty),f) then
            return {}
        end

        -- stop if at target tile
        if tx == tfx and ty == tfy then
            break
        end

        -- stop if distance limit reached
        dist += 1
        if dist >= md then
            return {}
        end

        local e2 = 2 * err
        if e2 > -dy then
            err -= dy
            tx += sx
        elseif e2 < dx then
            err += dx
            ty += sy
        end
    end

    return tiles
	end
	
	function o:draw()
			--draw_tile_line(self.tiles)
			spr(self.sprt,self.x,self.y)
			--draw_hitbox(self,11)
	end
	
	return o
end

