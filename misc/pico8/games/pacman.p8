-- utils --
anim=function(sf,d)
	return {sprt=d,sf=sf,si=1,sl=count(sf)}
end

updateanimation=function(o)
	if o==nil 
	 or o.si==nil
	 or o.sl==1 then
	 return o
	end
	
 if o.si<o.sl+0.3 then
  o.si+=0.1
 else
  o.si=1
 end
 
 o.sprt=o.sf[flr(o.si)] 
 return o
end
	
getsgn = function(v)
	return v==0 and 0 or sgn(v)
end

pointcollideflag=function(x,y,flag) 
  return fget(mget(x/8,y/8))==flag
end

flgmtch=function(t, fs)
  for f in all(fs) do
    if fget(t,f) then
      return true
    end
  end
  return false
end

collideflag=function(x,y,b,flags)
	 for i=x+b.x,x+b.x+b.w-1,b.w-1 do
    if flgmtch(mget(i/8,(y+b.y)/8),flags) or
         flgmtch(mget(i/8,(y+b.y+b.h-1)/8),flags) then
          return true
    end
  end
  for i=y+b.y,y+b.y+b.h-1,b.h-1 do
    if flgmtch(mget((x+b.x)/8,i/8),flags) or
         flgmtch(mget((x+b.x+b.w-1)/8,i/8),flags) then
          return true
    end
  end  
  return false
end

function rects_overlap(o1,o2)
		local o1x,o1y,o2x,o2y=
			o1.x+o1.box.x,
			o1.y+o1.box.y,
			o2.x+o2.box.x,
			o2.y+o2.box.y
		
  return o1x < o2x + o2.box.w and
         o1x + o1.box.w - 1 > o2x and
         o1y < o2y + o2.box.h and
         o1y + o1.box.h - 1 > o2y
end

function draw_hitbox(obj, col)
    col = col or 8 -- default color (red)
    rect(
        obj.x + obj.box.x,
        obj.y + obj.box.y,
        obj.x + obj.box.x + obj.box.w - 1,
        obj.y + obj.box.y + obj.box.h - 1,
        col
    )
end

-- draw tiles (as rects for debugging)
function draw_tile_line(tiles)
    -- draw all tiles
    for i=1,#tiles do
        local tile = tiles[i]
        rect(tile.x * 8, tile.y * 8, tile.x * 8 + 7, tile.y * 8 + 7, 10)
    end
end

-- timer object constructor
function make_timer(period)
  local o={
    period = period*30,--30fps
    t = 0    
  }
  
  function o:update()
    self.t += 1
    if self.t >= self.period then
        self.t -= self.period
        return true
    end
    return false
  end
  
  return o
end

function postotile(x,y)
		return {
			x=flr((x-mappos.x*16*8)/8),
			y=flr((y-mappos.y*16*8)/8)
		}		
end

