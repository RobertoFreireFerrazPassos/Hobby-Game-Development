--randon level
function createlevel()
		mappos={
				x=rnd({0,1,2,3,4,5,6,7}),
				y=rnd({0,1})
		}		
  local free_tiles = {}

  -- find all free tiles (no tilemap set)
  for y=1,14 do
    for x=1,14 do
    		if fget(mget(x+mappos.x*16,y+mappos.y*16)) != 1 and not (x >= 5 and x <= 9 and y >= 9 and y <= 14) then
		      add(free_tiles, {x=x, y=y})
	     end
    end
  end

  -- randomly place enemies on some of them
  for i=1,5+2*menu.selected do -- place 5 enemies (change as needed)
    if #free_tiles == 0 then break end
    
    local index = flr(rnd(#free_tiles)) + 1
    local tile = deli(free_tiles, index)
				add(enemies,newenemy(tile.x,tile.y,i))
		end
end



