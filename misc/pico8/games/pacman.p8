-- player
butarr={1,2,0,3,5,6,3,4,8,7,4,0,1,2,0}
butarr[0]=0
dirx={-1,1, 0,0,-0.7, 0.7,0.7,-0.7}
diry={ 0,0,-1,1,-0.7,-0.7,0.7, 0.7}
		
ply={
	dirsprt={
 	anim({1,2,3,4},1),
 	anim({17,18,19,20},17),
 	anim({5,6,7,8},5),
 	anim({1,2,3,4},1),
 	anim({5,6,7,8},5),
 	anim({5,6,7,8},5),
 	anim({17,18,19,20},17),
 	anim({1,2,3,4},1)
 },
 dirf={1,0,0,0,1,0,0,1},
 box={x=1,y=1,w=6,h=6},
 dir,
 lastdir,
 gameovertimer = make_timer(2),
}
	  
function ply:init()
		self.sprt=1
		self.x=7*8+mappos.x*16*8
		self.y=9*8+mappos.y*16*8
		self.can_move = true
	 self.touch_timer = 0
		self.lose_time = 3 * 30
		self.spd=1
end

function ply:getcenter()
		return {x=self.x+4,y=self.y+4}
end

function ply:createbit()
		self.bit = 0b1111
		
		if collideflag(self.x-1*self.spd,self.y,self.box,{0}) then
						-- cantmoveleft 
				self.bit=self.bit&0b1110
		end		 
		if collideflag(self.x+1*self.spd,self.y,self.box,{0}) then
				-- cantmoveright
				self.bit=self.bit&0b1101
		end		 
		if collideflag(self.x,self.y-1*self.spd,self.box,{0}) then
				-- cantmoveup
				self.bit=self.bit&0b1011
		end		
		if collideflag(self.x,self.y+1*self.spd,self.box,{0}) then
				-- cantmovedown
				self.bit=self.bit&0b0111
		end
end

function ply:control() 
		self.dir=butarr[btn()&self.bit]	
  if self.lastdir!=self.dir and self.dir>=5 then
	  self.x=flr(self.x)+0.5
	  self.y=flr(self.y)+0.5
	 end	 
	 if self.dir>0 then
	  self.x+=dirx[self.dir]*self.spd
	  self.y+=diry[self.dir]*self.spd
  end
	 self.lastdir=self.dir
end

function ply:anim()		
		if self.dir==0 then
	  	return
  end  
  self.sprt=updateanimation(self.dirsprt[self.dir]).sprt
end

function ply:enemycolision()
		local pos=postotile(self.x+4,self.y+4)
		
		-- stop if tile has enemy
		if grid[pos.y][pos.x]>0 then
					self.can_move=false
					return
		end
end

function ply:eat()
		local x,y=flr((self.x+4)/8),flr((self.y+4)/8)
		if fget(mget(x,y))!=1 then
				if mget(x,y)==16 then
					sfx(7)
				end
				mset(x,y,0)
		end
end

function ply:update()
  self:enemycolision()  
	  
		if not self.can_move then
			if self.gameovertimer:update() then
	    sfx(1)
	    changestate(menu)		    	
	 	end
			return
		end
		
		self:createbit()
		self:control()
		self:anim()
		self:eat()
end

function ply:draw()
		spr(self.sprt,self.x,self.y)
		--draw_hitbox(self,11)
end
