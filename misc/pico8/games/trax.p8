smk={}
function smk_make(x,y)	
		for i=1,3 do
     add(smk,new_smk(x,y))
  end
end

function smk_update() 
  for s in all(smk) do
     s:update()
  end
end

function smk_draw() 
  for s in all(smk) do
		  circfill(s.x,s.y,s.rad,s.clr)
		end
end

function new_smk(x,y)	
		local o = {x=x+4,y=y+4,
    dx=flr(rnd(2)-1),dy=flr(rnd(2)-1),
    rad=2,act=25,
   	clr=rnd({8,9,10})}
   	
  function o:update()
  		local s=self
  		s.x+=s.dx*0.5
    s.y+=s.dy*0.5
    s.act-=1
    if s.act<15 then s.rad=1 end
    if s.act<0 then
        del(smk,s)
    end
  end
  	
  return o
end
