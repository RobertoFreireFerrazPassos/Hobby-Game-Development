function game:update()
		foreach(obj, function(o)
				if o.update ~= nil then
						o:update()
				end
		end)		
		smk_update()
		update_camera(obj[1].b)
end

function game:draw()
  camera(cam_x, cam_y)
		map()
		foreach(obj, function(o)	
				if o.lsb ~= nil then
						foreach(o.lsb,function(s)
								s:draw()
						end)
				end							
				if o.b ~= nil then
						o.b:draw()
				end
				if o.lsa ~= nil then
						foreach(o.lsa,function(s)
								s:draw()
						end)
				end
		end)
		smk_draw()
end
