-- entities		
function new_player(s,x,y,z)
		local o={}		
		o.b=new_body(s,x,y,0,0,8,8,true,z)
		o.a=new_anim({1},1,false)
		o.butarr={1,2,0,3,5,6,3,4,8,7,4,0,1,2,0}
		o.butarr[0]=0
		o.dirx={-1,1, 0,0,-0.7, 0.7,0.7,-0.7}
		o.diry={ 0,0,-1,1,-0.7,-0.7,0.7, 0.7}
		o.drw=1 -- weapon_direction
		o.anim={ 
				new_anim({1,2,3},1,false),
				new_anim({3,4,5},1,false),
				new_anim({5,6,7},1,false),
				new_anim({7,8,9},1,false),
				new_anim({9,10,11},1,false),
				new_anim({11,12,13},1,false),
				new_anim({13,14,15},1,false),
				new_anim({15,16,1},1,false),
		}
		o.lwd=1 -- last_weapon_direction
		o.cwd=1 -- current_weapon_direction
		o.wtmr=15 -- weapon_timer
		-- ricochet of the weapon
		o.rwx={ 0,-1,-2,-1, 0, 1,2,1}
		o.rwy={ 2, 1, 0,-1,-2,-1,0,1}
		o.lsb={}  -- sprite list before
		o.lsa={} -- sprite list after
		o.cwi=1  -- current wheel animation
		o.whl={ -- wheels animation
				new_anim({19,20,23},3),
				new_anim({19,23,20},3),
				new_anim({19,23,22},3),
				new_anim({19,22,23},3),
				new_anim({19,20,20},3),
				new_anim({19,21,21},3),
				new_anim({19,22,22},3),
				new_anim({19,23,23},3),
		}
		o.lsb={
				new_sprite(19,o.b.x-3,o.b.y-3),
				new_sprite(19,o.b.x-3,o.b.y+3),
				new_sprite(19,o.b.x+3,o.b.y-3),
				new_sprite(19,o.b.x+3,o.b.y+3)
		}
		
		function o:control() 
					local s=self					
					s.dir=s.butarr[btn()&0b1111]
			  if s.lastdir!=s.dir and s.dir>=5 then
				  s.b.x=flr(s.b.x)+0.5
				  s.b.y=flr(s.b.y)+0.5
				 end	 
				 if s.dir>0 then
					  s.dx+=s.dirx[s.dir]					  
					  s.dy+=s.diry[s.dir]
					  s.cwi=s.dir
				 end
				 
				 if collideflag(s.b.x,s.b.y+s.dy,s.b.box,{1}) then	
								s.dy=0
					end
					
					if collideflag(s.b.x+s.dx,s.b.y,s.b.box,{1}) then	
								s.dx=0
					end
					
					s.b.x+=s.dx
					s.b.y+=s.dy					
					s.lastdir=s.dir
			end
			
			function o:anim_wheel()
					local s=self
					s.whl[s.cwi].done = s.dir<=0 
					local ws=s.whl[s.cwi]:update()
					
					s.lsb[1].x=s.b.x-3
				 s.lsb[1].y=s.b.y-3
				 s.lsb[1].s=ws
				 s.lsb[2].x=s.b.x-3
				 s.lsb[2].y=s.b.y+3
				 s.lsb[2].s=ws
				 s.lsb[3].x=s.b.x+3
				 s.lsb[3].y=s.b.y-3
				 s.lsb[3].s=ws
				 s.lsb[4].x=s.b.x+3
				 s.lsb[4].y=s.b.y+3
				 s.lsb[4].s=ws
			end
		
			function o:move_weapon()
				local s=self
	
		  if btnp(‚ùé) then
			    s.cwd+=s.drw			    
			 			if s.cwd>#s.anim then
			 					s.cwd=1
			 			elseif s.cwd<1 then
			 					s.cwd=#s.anim
			 			end
			 end
				
				if s.lwd~=s.cwd then
							s.b.s=s.anim[s.cwd]:update()
							if s.anim[s.cwd].done then
										s.lwd=s.cwd
							end
				end
		end
		
		function o:shoot()
				local s=self
				if s.wtmr > 0 then
							s.wtmr -= 1
				end
	
				if btn(üÖæÔ∏è) and s.wtmr <= 0 then
						local dirbullet = mid(1,flr(s.b.s/2)+1,8)
						new_bullet(s.b.x,s.b.y,dirbullet)
						s.wtmr=15
						s.dx=s.rwx[dirbullet]
						s.dy=s.rwy[dirbullet]
						sfx(rnd({0,1}))
				end
		end
		
		function o:update()
			local s=self
			s.dx=0
			s.dy=0
			s:move_weapon() 
			s:shoot()
			s:control()
			s:anim_wheel()
		end
		
		return o
end

function new_bullet(x,y,dir)
		local o = {}
		o.dir=dir
		o.dirx={ 0, 0.7,1,0.7,0,-0.7,-1,-0.7}
		o.diry={-1,-0.7,0,0.7,1, 0.7, 0,-0.7}
		o.tmr=0	
		o.b=new_body(32,x,y,2,2,4,4,true,0)
		o.b.x+=o.dirx[o.dir]*4
		o.b.y+=o.diry[o.dir]*4
		smk_make(o.b.x,o.b.y)
		
		function o:update()
				local s=self
				s.b.x+=s.dirx[s.dir]*4
				s.b.y+=s.diry[s.dir]*4
				s.tmr+=1
				s.b.s=rnd({32,33,34})
				
				if s.tmr>10 then 
						del(obj,s)
				elseif collideflag(s.b.x,s.b.y,s.b.box,{0}) then
						del(obj,s)
						smk_make(s.b.x,s.b.y)
				end
		end
		
		add(obj,o)
		return o		
end

